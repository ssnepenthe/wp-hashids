{
    "name": "ssnepenthe/wp-hashids",
    "description": "A Hashids implementation for WordPress",
    "type": "wordpress-plugin",
    "homepage": "https://github.com/ssnepenthe/wp-hashids",
    "license": "GPL-2.0",
    "require": {
        "php": "^7.4 || ^8.0",
        "composer/installers": "^1.0",
        "hashids/hashids": "^2.0 || ^3.0 || ^4.0",
        "league/plates": "^3.3",
        "ssnepenthe/daedalus": "dev-main",
        "ssnepenthe/daedalus-pimple": "dev-master",
        "ssnepenthe/wp-requirements": "^0.1"
    },
    "require-dev": {
        "mockery/mockery": "^0.9",
        "psy/psysh": "^0.8"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../daedalus"
        },
        {
            "type": "path",
            "url": "../daedalus-pimple"
        },
        {
            "type": "path",
            "url": "../toy-wp-event-management"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
